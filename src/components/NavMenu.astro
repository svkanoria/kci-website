---
import { PageLink } from "../utils";
import NavMenuFab from "./NavMenuFab.astro";
import NavMenuList from "./NavMenuList.astro";

interface Props {
  pageLinks: Record<string, PageLink[]>;
  isCompact?: boolean;
  className?: string;
}

const { className, ...props } = Astro.props;

export type NavMenuProps = Props;
---

<NavMenuFab className="lg:hidden" {...props} />
<NavMenuList className="max-md:hidden" {...props} />
