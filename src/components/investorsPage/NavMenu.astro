---
import { concatPageLink, getPageLinks } from "../../utils";
import NavMenu from "../NavMenu.astro";

const links = await getPageLinks(
  import.meta.glob("../../pages/investors/*.{astro,md,mdx,html}")
);
---

<NavMenu
  pageLinks={concatPageLink(links, { href: "/investors", title: "Overview" })}
  isCompact
/>
